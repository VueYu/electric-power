el=\"运行容量\">\n                                <span>{{ props.row.value8 }}</span>\n                            </el-form-item>\n                            <el-form-item label=\"供电单位\">\n                                <span>{{ props.row.value9 }}</span>\n                            </el-form-item>\n                            <el-form-item label=\"抄表段号\">\n                                <span>{{ props.row.value10 }}</span>\n                            </el-form-item>\n                            <el-form-item label=\"电压等级\">\n                                <span>{{ props.row.value11 }}</span>\n                            </el-form-item>\n                            <el-form-item label=\"台区名称\">\n                                <span>{{ props.row.value12 }}</span>\n                            </el-form-item>\n                            <el-form-item label=\"台区编码\">\n                                <span>{{ props.row.value13 }}</span>\n                            </el-form-item>\n                            <el-form-item label=\"测点类型\">\n                                <span>{{ props.row.value14 }}</span>\n                            </el-form-item>\n                            <el-form-item label=\"台区性质\">\n                                <span>{{ props.row.value15 }}</span>\n                            </el-form-item>\n                            <el-form-item label=\"台区运行状态\">\n                                <span>{{ props.row.value16 }}</span>\n                            </el-form-item>\n                        </el-form>\n                    </template>\n                </el-table-column>\n                <el-table-column prop=\"value1\" label=\"供电单位\" width=\"200\">\n                </el-table-column>\n                <el-table-column prop=\"value2\" label=\"供电线路\" width=\"200\">\n                </el-table-column>\n                <el-table-column prop=\"value3\" label=\"台区名称\" width=\"300\">\n                </el-table-column>\n                <el-table-